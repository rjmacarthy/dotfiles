alias gp="git push origin HEAD"
alias gf="git fetch upstream && git fetch origin"
alias gld="git log upstream/staging..HEAD"
alias grus="git rebase upstream/staging"
alias grum="git rebase upstream/master"
alias gruma="git rebase upstream/main"
alias gsa0="git stash apply stash@{0}"
alias gro="git reset --hard origin/staging"
alias gru="git reset --hard upstream/staging"
alias grm="git reset --hard upstream/master"
alias grma="git reset --hard upstream/main"
alias grh="git reset --hard"
alias gcm="git checkout master"
alias gcs="git checkout staging"
alias gsui="git submodule update --init"
alias gpf="git push -f origin HEAD"
